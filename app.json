{
  "expo": {
    "name": "DriverApp",
    "slug": "DriverApp",
    "version": "1.0.0",
    "scheme": "driverapp",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.driverapp.driverapp",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to your location for ride tracking.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to your location even when not in use for ride tracking.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCmYI_xnBdw-FRIp6FjkulduUNte8n3FWk"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.driverapp.driverapp",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.VIBRATE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCmYI_xnBdw-FRIp6FjkulduUNte8n3FWk"
        }
      },
      "notification": {
        "channels": [
          {
            "name": "Batch Notifications",
            "id": "batch",
            "description": "Notifications for batch updates",
            "sound": true,
            "vibration": [0, 250, 250, 250],
            "importance": "high",
            "visibility": "public",
            "vibrate": true,
            "showBadge": true
          }
        ]
      },
      "foregroundService": {
        "types": ["location"]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-task-manager",
      "expo-background-fetch",
      [
        "expo-location",
        {
          "isIosBackgroundLocationEnabled": true,
          "locationAlwaysAndWhenInUsePermission": "Allow DriverApp to access your location for ride tracking."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "defaultChannel": "batch"
        }
      ],
      [
        "expo-task-manager",
        {
          "taskManager": {
            "socket-background-task": "socketBackgroundTask",
            "location-background-task": "locationBackgroundTask"
          }
        }
      ],
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d54b9de8-0006-44a8-b2e1-6c123804e0fc"
      }
    },
    "owner": "mosessaleh"
  }
}
